{
  "name": "vue-zero-admin",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:prod": "run-p \"build-only --mode production\" --",
    "build:test": "run-p \"build-only --mode test\" --",
    "build:dev": "run-p \"build-only --mode development\" --",
    "preview": "vite preview",
    "build-only": "vite build --mode production",
    "type-check": "vue-tsc --build --force",
    "lint": "npx oxlint && npx eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@vueuse/core": "^12.0.0",
    "axios": "^1.7.9",
    "element-plus": "^2.9.1",
    "es-toolkit": "^1.30.1",
    "pinia": "^2.3.0",
    "vue": "^3.5.13",
    "vue-i18n": "^10.0.5",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^6.0.1",
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.2",
    "@unocss/eslint-config": "0.65.1",
    "@unocss/preset-rem-to-px": "0.65.1",
    "@unocss/transformer-directives": "0.65.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.17.0",
    "eslint-plugin-oxlint": "^0.15.1",
    "eslint-plugin-vue": "^9.32.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^7.0.1",
    "oxlint": "^0.15.2",
    "prettier": "^3.4.2",
    "sass": "^1.83.0",
    "typescript": "~5.7.2",
    "unocss": "0.65.1",
    "unplugin-auto-import": "^0.19.0",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-vue-components": "^0.27.5",
    "vite": "6.0.3",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-mkcert": "^1.17.6",
    "vite-plugin-oxlint": "^1.1.0",
    "vite-plugin-pages": "^0.32.4",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "^7.6.8",
    "vue-tsc": "^2.1.10"
  },
  "lint-staged": {
    "**/*.{js,mjs,cjs,jsx,ts,mts,cts,tsx,vue,astro,svelte}": "oxlint"
  },
  "types": "./types/index.d.ts"
}
