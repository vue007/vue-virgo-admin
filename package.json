{
  "name": "vue-zero-admin",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:prod": "run-p \"build-only --mode production\" --",
    "build:test": "run-p \"build-only --mode test\" --",
    "build:dev": "run-p \"build-only --mode development\" --",
    "preview": "vite preview",
    "build-only": "vite build --mode production",
    "type-check": "vue-tsc --build --force",
    "lint": "npx oxlint && npx eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@vueuse/core": "^11.1.0",
    "axios": "^1.7.7",
    "disable-devtool": "^0.3.8",
    "element-plus": "^2.8.8",
    "es-toolkit": "^1.25.2",
    "pinia": "^2.1.7",
    "vue": "^3.5.13",
    "vue-i18n": "10",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^6.0.0",
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.7.7",
    "@unocss/eslint-config": "^0.64.1",
    "@unocss/preset-rem-to-px": "^0.64.1",
    "@unocss/transformer-directives": "^0.64.1",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/eslint-config-prettier": "^10.0.0",
    "@vue/eslint-config-typescript": "^14.1.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.6.0",
    "eslint": "^9.15.0",
    "eslint-plugin-oxlint": "^0.11.1",
    "eslint-plugin-vue": "^9.29.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^7.0.1",
    "oxlint": "^0.11.1",
    "prettier": "^3.2.5",
    "sass": "^1.81.0",
    "typescript": "~5.6.3",
    "unocss": "^0.64.1",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-vue-components": "^0.27.4",
    "vite": "6.0.0-beta.10",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-mkcert": "^1.17.6",
    "vite-plugin-oxlint": "^1.1.0",
    "vite-plugin-pages": "^0.32.3",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "^7.5.2",
    "vue-tsc": "^2.0.21"
  },
  "lint-staged": {
    "**/*.{js,mjs,cjs,jsx,ts,mts,cts,tsx,vue,astro,svelte}": "oxlint"
  }
}